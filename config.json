{
  "$schema": "https://www.getbifrost.ai/schema",
  "concurrency": 1000,
  "client": {
    "drop_excess_requests": false
  },
  "governance": {
    "enabled": true
  },
  "logging": {
    "enabled": true
  },
  "storage": {
    "mode": "postgres",
    "config_store": {
      "enabled": true,
      "type": "postgres",
      "connection_string": "env.DATABASE_URL"
    },
    "logs_store": {
      "enabled": true,
      "type": "postgres",
      "connection_string": "env.DATABASE_URL"
    }
  },
  "providers": {
    "openrouter": {
      "enabled": true,
      "keys": [
        {
          "name": "primary-openrouter",
          "value": "env.OPENROUTER_API_KEY",
          "weight": 1.0
        }
      ]
    }
  }
}